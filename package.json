{
  "name": "fe-service-desktop-sample",
  "version": "1.0.0",
  "private": true,
  "description": "Service Desktop Sample",
  "scripts": {
    "start": "webpack serve --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "build-hotfix": "webpack --config webpack.hotfix.js",
    "defs": "cd defsGetter && node index.js",
    "clean": "rm -rf dist",
    "eslint": "eslint --ext .tsx,.ts ./src",
    "eslint-fix": "eslint --fix --ext .tsx,.ts ./src",
    "prettier": "prettier --write --ignore-unknown .",
    "test-watch": "jest --watch",
    "test": "jest --clearCache && jest --coverage --maxWorkers=50% --silent",
    "test-ci": "jest --clearCache --cacheDirectory /tmp/${npm_package_name} && jest --coverage --maxWorkers=50% --silent --cacheDirectory /tmp/${npm_package_name}",
    "security": "better-npm-audit audit --registry=\"https://registry.npmjs.org/\"",
    "security-fix": "npm audit fix --registry=\"https://registry.npmjs.org/\"",
    "code-quality": "npm run eslint",
    "ts-quality": "tsc --noEmit --skipLibCheck",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.22.17",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@swc/core": "^1.3.19",
    "@swc/jest": "^0.2.23",
    "@swipejobs/s3-build-artifact-webpack-plugin": "^2.0.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^28.1.0",
    "@types/lodash-es": "^4.17.9",
    "@types/single-spa-react": "^4.0.0",
    "@types/webpack": "^5.28.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-jest": "^28.1.3",
    "babel-loader": "^9.1.0",
    "better-npm-audit": "^3.7.3",
    "eslint": "^8.49.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^26.9.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-sonarjs": "^0.21.0",
    "fork-ts-checker-webpack-plugin": "^7.3.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-sonar-reporter": "^2.0.0",
    "jest-watch-typeahead": "^1.1.0",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.0",
    "react-test-renderer": "^17.0.1",
    "recursive-readdir-sync": "^1.0.6",
    "ts-jest": "^28.0.8",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.9.5",
    "webpack": "^5.88.2",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.61",
    "@sentry/react": "^6.19.7",
    "@swipejobs/react-hooks": "^2.16.4",
    "i18next": "^23.7.19",
    "lodash-es": "^4.17.21",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.18.6",
    "single-spa": "^5.9.5",
    "single-spa-react": "^4.6.1"
  }
}
